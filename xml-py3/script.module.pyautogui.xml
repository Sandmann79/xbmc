<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<addon id="script.module.pyautogui"
	name="PyAutoGUI"
	version="{ver_add}"
	provider-name="Al Sweigart">
	<requires>
		<import addon="xbmc.python" version="{ver_py}"/>
	</requires>
	<extension point="xbmc.python.module" library="lib" />
	<extension point="xbmc.addon.metadata">
		<summary lang="en">PyAutoGUI is a cross-platform GUI automation Python module for human beings. Used to programmatically control the mouse and keyboard.</summary>
		<forum></forum>
		<website>https://pyautogui.readthedocs.io/</website>
		<license>see LICENSE.txt</license>
		<platform>all</platform>
		<source>https://github.com/asweigart/pyautogui</source>
	</extension>
</addon>
